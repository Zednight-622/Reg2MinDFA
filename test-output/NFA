	graph [color=lightgrey rankdir=LR style=filled]
	node [shape=doublecircle]
	10 [label=10]
	node [shape=circle]
	0 -> 1 [label="ε"]
	1 -> 2 [label="ε"]
	2 -> 2 [label=a]
	2 -> 3 [label="ε"]
	3 -> 4 [label="ε"]
	4 -> 5 [label=a]
	5 -> 6 [label=b]
	6 -> 4 [label=b]
	4 -> 7 [label="ε"]
	7 -> 8 [label=b]
	8 -> 9 [label="ε"]
	9 -> 9 [label=b]
	9 -> 9 [label=a]
	9 -> 10 [label="ε"]
}
